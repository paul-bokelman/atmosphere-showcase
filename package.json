{
  "name": "atmosphere-showcase",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "dotenv -e .env -- prisma generate --schema ./prisma/schema.prisma",
    "db:push": "dotenv -e .env -- prisma db push --schema ./prisma/schema.prisma",
    "db:reset": "dotenv -e .env -- prisma migrate reset --schema ./prisma/schema.prisma",
    "db:generate:prod": "dotenv -e .env.production -- prisma generate --schema ./prisma/schema.prisma",
    "db:push:prod": "dotenv -e .env.production -- prisma db push --schema ./prisma/schema.prisma",
    "db:reset:prod": "dotenv -e .env.production -- prisma migrate reset --schema ./prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.5.1",
    "framer-motion": "^11.3.8",
    "next": "14.2.5",
    "nprogress": "^0.2.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "react-use": "^17.5.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "prisma": "^5.17.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
